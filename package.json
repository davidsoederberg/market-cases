{
  "name": "market-case",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "DEBUG=b-rscase:* node ./bin/www",
    "lint": "eslint . --ext .js --ext .json && cd ./frontend && npm run lint",
    "pretest": "npm run lint",
    "test": "DEBUG=b-rscase:test:* NODE_ENV=test nyc mocha --timeout 5000 --exit --recursive && cd ./frontend && npm run test:unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "cd ./frontend && npm run serve",
    "build": "cd ./frontend && npm run build"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "moment": "^2.23.0",
    "mongoose": "^5.4.1",
    "morgan": "~1.9.0",
    "node-cron": "^2.0.3",
    "pug": "2.0.0-beta11",
    "request": "^2.88.0",
    "stocks.js": "github:davidsoederberg/stocks.js"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-vue": "^5.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  }
}
